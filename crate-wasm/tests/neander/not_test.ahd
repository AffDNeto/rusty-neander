; Testa not
; NOT A, N=1
LDA ZERO; AC = 0
NOT     ; AC = !0 = 255(-1)
STA T0
JZ ERR 	
JN P1
JMP ERR
P1:
ADD ONE ; AC = -1 + 1 = 0
JZ P2
JMP ERR

; NOT FF = 00, e Z=0 
P2:
LDA MAX	; AC = 255
NOT     ; AC = !255 = 0
STA T1
JN ERR	
JZ P3
JMP ERR

; NOT(NOT A) = A
P1:
LDA ZERO; AC = 0
NOT
NOT
STA T2
JN ERR	
JZ END

ERR: 
LDA ONE
STA E
END:
HLT

ORG 128
T0: 1
T1: 1
T2: 1
ZERO: 0	
ONE: 1
MAX: 255; Valor maximo em byte
MAXN:127; Valor maximo em complemento de 2
MINN:128; Valor minimo negativo em complemento de 2	
E: 0    ; Variavel para indicar que teve um erro no teste