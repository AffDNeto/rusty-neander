; Testa not
; no test can change the V,C, and B flags so they are set to 0 before testing
LDA ZERO
ADD ZERO
SUB ZERO

; NOT 00 = FF
LDA ZERO; AC = 0
NOT     ; AC = !0 = 255(-1)
STA T0
JZ ERR
JC ERR
JB ERR
JV ERR 	
JP ERR
ADD ONE ; AC = -1 + 1 = 0
JZ P2
JMP ERR

; NOT FF = 00
P2:
LDA MAX	; AC = 255
NOT     ; AC = !255 = 0
STA T1
JN ERR
JC ERR
JB ERR
JV ERR	
JZ P3
JMP ERR

; NOT(NOT A) = A
P1:
LDA ZERO; AC = 0
NOT
NOT
STA T2
JN ERR
JC ERR
JB ERR
JV ERR	
JZ END

END:
LDA ZERO
STA E
ERR: 
HLT

ORG 128
T0: 1
T1: 1
T2: 1
ZERO: 0	
ONE: 1
MAX: 255; Valor maximo em byte
MAXN:127; Valor maximo em complemento de 2
MINN:128; Valor minimo negativo em complemento de 2	
E: 1    ; Variavel para indicar que teve um erro no teste