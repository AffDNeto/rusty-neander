; Testes NOT
CLR R0
CCC ZNCV

JMP START
JMP_ERR:
JMP ERR
START:
; FLAGS
INC R0
CLR R1
SCC ZV
CCC NC
NOT R1
BCC JMP_ERR
BVS JMP_ERR
BEQ JMP_ERR
BPL JMP_ERR
CMP R1 TRUE
BNE JMP_ERR

; NOT TRUE = FALSE
INC R0
MOV TRUE R1
NOT R1
BCC ERR
BVS ERR
CMP R1 FALSE
BNE ERR

; NOT FALSE = TRUE
INC R0
MOV FALSE R1
NOT R1
BCC ERR
BVS ERR
CMP R1 TRUE
BNE ERR

; NOT NOT A = A
INC R0
MOV A R1
NOT R1
BCC ERR
BVS ERR
CMP R1 NOT_A
BNE ERR
NOT R1
BCC ERR
BVS ERR
CMP R1 A
BNE ERR

; MODOS DE ENDERECAMENTO
;PRE DEC
MOV #T R1
NOT -(R1)   ; escreve em T_PRE_DEC
;POS INC
NOT #0   ; mesmo que INC (R7)+
;INDIRETO
MOV #T R1
NOT (R1)    ; escreve em T
;INDEXADO
NOT 2(R1)   ; escreve em T_IDX
;IND POS INC
MOV #TI_POS_INC TMP
MOV #TMP R1
NOT ((R1)+)
;IND PRE DEC
MOV #TI_PRE_DEC TMP
NOT (-(R1)) ; escreve em TI_PRE_DEC
;IND INDEXADO
MOV #TI_IDX R1
INC R1
INC R1
NOT (-2(R1))   ; escreve em TI_IDX

JMP END
ERR:
MOV #1 E
END:
MOV R0 COUNTER
HLT

ORG 1024
E: 0
COUNTER: 0
FALSE: H0000
TRUE: HFFFF
A: HF0F0
NOT_A: H0F0F
T_PRE_DEC: 1
T: 2
T_IDX:3
TI_POS_INC: 5
TI_PRE_DEC:6
TI_IDX: 7
TMP: 8