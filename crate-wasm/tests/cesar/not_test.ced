; Testes NOT
CLR R0
CCC ZNCV

; FLAGS
SCC V
CCC C
INC R0
CLR R1
NOT R1
BCC ERR
BVS ERR
BEQ ERR
BPL ERR
CMP R1 TRUE
BNE ERR

; NOT TRUE = FALSE
INC R0
MOV TRUE R1
NOT R1
CMP R1 FALSE
BNE ERR

; NOT FALSE = TRUE
INC R0
MOV FALSE R1
NOT R1
CMP R1 TRUE
BNE ERR

; NOT NOT A = A
INC R0
MOV A R1
NOT R1
CMP R1 NOT_A
BNE ERR
NOT R1
CMP R1 A
BNE ERR

JMP END
ERR:
MOV R0 COUNTER
MOV #1 E
END:
HLT

ORG 1024
E: 0
COUNTER: 0
FALSE: H0000
TRUE: HFFFF
A: HF0F0
NOT_A: H0F0F