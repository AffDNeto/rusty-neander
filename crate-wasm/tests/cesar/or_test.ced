; Testes OR
CLR R0
CCC ZNCV

; FLAGS
INC R0
MOV TRUE R1
OR A R1    ; 0xF0F0 or 0xFFFF = 0xF0F0
BEQ ERR
BPL ERR
BCS ERR
BVS ERR

; A or A = A
INC R0
MOV A R1
OR A R1
CMP R1 A
BNE ERR

; A or TRUE = TRUE
INC R0
MOV A R1
OR TRUE R1
CMP R1 TRUE
BNE ERR

; A or FALSE = A
INC R0
MOV A R1
OR FALSE R1
CMP R1 A
BNE ERR

; A or (NOT A) = TRUE
INC R0
MOV A R1
OR NOT_A R1
CMP R1 TRUE
BNE ERR

JMP END
ERR:
MOV R0 COUNTER
MOV #1 E
END:
HLT

ORG 1024
E: 0
COUNTER: 0
FALSE: H0000
TRUE: HFFFF
A: HF0F0
NOT_A: H0F0F