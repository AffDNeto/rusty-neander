; Testes AND
CLR R0
CCC ZNCV

; FLAGS
INC R0
MOV TRUE R1
AND A R1    ; 0xF0F0 and 0xFFFF = 0xF0F0
BEQ ERR
BPL ERR
BCS ERR
BVS ERR

; A and A = A
INC R0
MOV A R1
AND A R1
CMP R1 A
BNE ERR

; A and TRUE = A
INC R0
MOV A R1
AND TRUE R1
CMP R1 A
BNE ERR

; A and FALSE = FALSE
INC R0
MOV A R1
AND FALSE R1
CMP R1 FALSE
BNE ERR

; A and (NOT A) = FALSE
INC R0
MOV A R1
AND NOT_A R1
CMP R1 FALSE
BNE ERR

JMP END
ERR:
MOV R0 COUNTER
MOV #1 E
END:
HLT

ORG 1024
E: 0
COUNTER: 0
FALSE: H0000
TRUE: HFFFF
A: HF0F0
NOT_A: H0F0F